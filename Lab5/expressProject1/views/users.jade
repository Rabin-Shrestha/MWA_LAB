extends layout

block content
  h1= title
  p Welcome to #{title}
     table
            tr
                th Id
                th Name
                th UserName
                th Email
                th City
                th "Zip Code"
                th Latitude
                th Longitude
                th Website

            for user in allData.dataFromPromisify
                tr
                    td: !{user.id}
                    td: !{user.name}
                    td: !{user.username}
                    td: !{user.email}
                    td: !{user.address.city}
                    td: !{user.address.zipcode}
                    td: !{user.address.geo.lng}
                    td: !{user.address.geo.lat}
                    td: !{user.website}


    table
        tr
            th Id
            th Name
            th UserName
            th Email
            th City
            th "Zip Code"
            th Latitude
